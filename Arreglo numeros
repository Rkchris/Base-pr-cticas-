#incluede "iostream" 
Using namespace std ;
Int main() {
Const Int tan arreglo=5;
Int números [tanarreglo];
For(Int i=0;<tanarreglo;i++){
Cout<<"ingrese el numero"<<i+1<<":";
Cin>>numero[i];
}
